<div class="card my-4">
    <h5 class="card-header">Search</h5>
    <div class="card-body">
        {{ form_start(form) }}
        <div class="input-group">
            {{ form_widget(form.search) }}
            <span class="input-group-append">
                  {{ form_widget(form.submit) }}
            </span>
        </div>
        {{ form_end(form) }}
    </div>
</div>